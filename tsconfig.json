{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "lib": [ "es2020", "dom" ],
    "declaration": true,
    "outDir": "./dist/",
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
